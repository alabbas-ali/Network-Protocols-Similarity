#function definition
# ( CommonStand.dev, differance_of_mean ,confidanceLevel, ß error, degrees of freedom)
sample_Size<-function(Sp ,d, a, b ,f)
{
n <- (qt(a,f) + qt(b,f))^2 * Sp^2 * (2/d^2)
for (m in ceiling(n):100) {
df = 2*m - 2
k <- (qt(a,df) + qt(b,df))^2 * Sp^2 * (2/d^2)
if (m >= k) {
return(m)
}
}
}
